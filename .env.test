NODE_ENV=production
# Whether to open mock
VITE_USE_MOCK = true

# public path
VITE_PUBLIC_PATH = /

# Whether to enable gzip or brotli compression
# Optional: gzip | brotli | none
# If you need multiple forms, you can use `,` to separate
VITE_BUILD_COMPRESS = 'none'

# Basic interface address SPA
VITE_GLOB_API_URL=/basic-api

# Interface prefix
VITE_GLOB_API_URL_PREFIX=

# Interface prefix of auth
VITE_GLOB_API_AUTH_URL_PREFIX=/auth

# Interface prefix of upms
VITE_GLOB_API_UPMS_URL_PREFIX=/upms

# Interface prefix of ticket
VITE_GLOB_API_TICKET_URL_PREFIX=/ticket
